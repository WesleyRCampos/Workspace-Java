
public class TestaPontoFluante {
	
	public static void main(String[] args) {
		double salario;
		salario = 1250.70;
		System.out.println("meu salário é " + salario);
		
		double idade= 37;
		
		double divisão = 3.14 / 2;
		System.out.println(divisão);
		
		int outraDivisao = 5 / 2;
		System.out.println(outraDivisao);
		
		double novaTenatativa = 5 / 2;
		System.out.println(novaTenatativa);
	}

}
